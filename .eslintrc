{
  "env": {
  },

  "globals": {
    "define": true,
    "$": true,
    "angular": true,
    "PubSub": true,
    "module": true,
    "require": true,
    "NODE_ENV": true,
    "process": true
  },
  "rules": {
    "no-bitwise": 2,
    "camelcase": [
        2, {
            "properties": "never"
        }
    ],
    "curly": [
        2,
        "all"
    ],
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-extend-native": 2,
    "wrap-iife": 2,
    "no-use-before-define": 2,
    "complexity": [
        2,
        10
    ],
    "max-depth": [
        2,
        5
    ],
    "max-len": [
        2,
        120
    ],
    "max-params": [
        2,
        10
    ],
    "max-statements": [
        2,
        20
    ],
    "new-cap": 2,
    "no-caller": 2,
    "no-empty": 2,
    "no-irregular-whitespace": 2,
    "no-new": 2,
    "quotes": [
        2,
        "single"
    ],
    "strict": [
        2,
        "never"
    ],
    "no-undef": 2,
    "no-unused-vars": 2,
    "indent": [
        2,
        2, {
            "SwitchCase": 1
        }
    ],
    "space-before-function-paren": [
        2,
        "never"
    ],
    "array-bracket-spacing": [
        2,
        "never"
    ],
    "space-in-parens": [
        2,
        "never"
    ],
    "quote-props": [
        2,
        "as-needed"
    ],
    "key-spacing": [
        2, {
            "beforeColon": false,
            "afterColon": true
        }
    ],
    "space-unary-ops": 0,
    "no-mixed-spaces-and-tabs": 2,
    "no-trailing-spaces": 2,
    "comma-dangle": [
        2,
        "never"
    ],
    "yoda": [
        2,
        "never"
    ],
    "no-with": 2,
    "no-multiple-empty-lines": 2,
    "space-before-blocks": [
        2,
        "always"
    ],
    "one-var": [
        2,{
          "var": "always",
          "let": "always",
          "const": "never"
        }
    ],
    "comma-style": [
        2,
        "last"
    ],
    "space-infix-ops": 2,
    "eol-last": 2,
    "dot-notation": 2,
    "space-return-throw-case": 0,
    "space-after-keywords": [
        2,
        "always"
    ],
    "consistent-this": [
        2,
        "_this"
    ],
    "linebreak-style": [
        2,
        "unix"
    ]
  }
}
